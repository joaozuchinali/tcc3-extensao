{
    "name": "Unesc TCC3: Extensão para coleta de dados",
    "version": "1.0.0",
    "description": "Teste",
    "author": "João Vitor Zucchinali Ghislandi",
    "action": {
        "default_popup": "index.html",
        "default_title": "Configurar acessos"
    },
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "userScripts",
        "topSites",
        "tabGroups",
        "history",
        "webNavigation",
        "webRequest"
    ],
    "host_permissions": [
        "http://*",
        "https://*"
    ],
    "background": {
        "service_worker": "./scripts/worker/service-worker.js",
        "type": "module"
      }
}